<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/xmlschema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemalocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name></display-name>
	<servlet>
		<servlet-name>monitorinfoservlet</servlet-name>
		<servlet-class>com.richstonedt.cmszbsszcst.smpcore.monitorweb.servlet.MonitorInfoServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>monitorinfoservlet</servlet-name>
		<url-pattern>/servlet/monitorinfoservlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TaskqueueServlet</servlet-name>
		<servlet-class>com.richstonedt.cmszbsszcst.smpcore.monitorweb.servlet.TaskqueueServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TaskqueueServlet</servlet-name>
		<url-pattern>/servlet/taskqueueServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>